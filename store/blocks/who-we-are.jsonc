  "flex-layout.row#historyCard":{
    "children":["slider-layout#card"],
    "props":{
      "blockClass": "slider"
    }
  },
  "slider-layout#card":{
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 2
      },
      "infinite": false,
      "showNavigationArrows": "always",
      "blockClass": "carousel",
      "centermode": "to-the-left",
      "usePagination":"true",
      "showPaginationDots": "never"
    },
    "children":["flex-layout.col#card01",
      "flex-layout.col#card02",
      "flex-layout.col#card03",
      "flex-layout.col#card04",
      "flex-layout.col#card05",
      "flex-layout.col#card06",
      "flex-layout.col#card07",
      "flex-layout.col#card08",
      "flex-layout.col#card09"
    ]
  },
  "flex-layout.col#card01":{
    "children":["image#logoCard01",
      "rich-text#yearCard01",
      "rich-text#descCard01"
    ],
    "props":{
      "blockClass": "containerCard"
    }
  },
  "image#logoCard01":{
    "props":{
      "src":"https://www.corebiz.ag/wp-content/uploads/2020/06/santa-monica.png",
      "blockClass": "imageCard"
    }
  },
  "rich-text#yearCard01":{
    "props":{
      "text": "2013"
    }
  },
  "rich-text#descCard01":{
    "props":{
      "text":"nasce a sta_monica, produtora digital focada em ecommerce. \n \n \\* o primeiro projeto de ecommerce foi feito de graça para conquistar um case.",
