{
  "store.custom#who-we-are": {
    "blocks": [
      "rich-text#title",
      "rich-text#subtitle",
      //"flex-layout.row#seeMore",
      "flex-layout.row#historyCard"      
    ],
    "parent": {
      "header": "header#vazio"
    }
  },
  "rich-text#title":{
    "props":{
      "text":"who we are?",
      "blockClass":"title"
    }
  },
  "rich-text#subtitle":{
    "props":{
      "text":"from santa monica \n to corebiz",
      "blockClass":"subtitle"
    }
  },
